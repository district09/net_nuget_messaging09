{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Broker": {
    "Username": "artemisuser",
    "Password": "artemispassword",
    "Uri": "amqp://localhost:5672",
    "TimeoutSeconds": 10
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Elastic.Apm": "Error"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {SourceContext} {TraceId} {CorrelationId} {Message:lj} {NewLine}{Exception}"
        }
      }
    ],
    "Properties": {
      "Application": "District09.Messaging.Example.WebApi",
      "ServiceType": "District09.Messaging.Example.WebApi",
      "ServiceName": "District09.Messaging.Example.WebApi",
      "DigipolisSubcel": [
        "ont"
      ]
    }
  }
}
